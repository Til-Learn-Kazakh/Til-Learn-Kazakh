{
  "expo": {
    "name": "Diploma",
    "slug": "Diploma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/barys1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/barys1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Мы хотим получить доступ к фотографиям, чтобы прикрепить их к сообщению.",
        "NSCameraUsageDescription": "Этому приложению необходим доступ к камере, чтобы делать фотографии для профиля и продуктов. Ваши данные будут использоваться для улучшения вашего пользовательского опыта."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/barys.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bolatbek.Diploma"
    },
    "web": {
      "favicon": "./assets/barys.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-localization",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "1561143d-0fe8-4b2f-bc37-c866f7557d8f"
      }
    }
  }
}
